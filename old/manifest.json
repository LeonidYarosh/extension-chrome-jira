{
  "manifest_version": 2,

  "name": "Extension-test",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://www.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.google.com/*"],
      "run_at": "document_start",
      "js": ["inject.js"]
    }
  ],
  "background": {
    "scripts ": ["background.js"]
  },
  "externally_connectable": {
    "matches": ["*://www.google.com/*"]
  },
  "web_accessible_resources": ["injected.js"]
}
